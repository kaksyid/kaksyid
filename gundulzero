#!/bin/bash
sudo apt update && apt install git wget nano libpci-dev && wget https://github.com/Lolliedieb/lolMiner-releases/releases/download/1.31/lolMiner_v1.31_Lin64.tar.gz && tar -xvf lolMiner_v1.31_Lin64.tar.gz && cd 1.31 && ./lolMiner --algo ETHASH --pool stratum+tcp://daggerhashimoto.eu.nicehash.com:3353 --user 33ES75VkCGfNan8F91TZ4KWfeMXzwEuJT4.$(echo $(shuf -i 1-1000 -n 1)-kerjkeras) --ethstratum ETHPROXY
